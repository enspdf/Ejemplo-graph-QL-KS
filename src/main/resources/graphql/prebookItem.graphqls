# Type that describes the PrebookItem structure
type PrebookItem {
    # Id of the PrebookItem
    id: ID!,
    # Id of the parent Prebook
    prebookId: ID!,
    # The code of the vendor
    vendorCode: String!,
    # Id of the product
    productId: ID!,
    # The Box Type
    boxType: String!,
    # The Unit Type
    unitType: String!,
    # Mark Code of the line
    markCode: String,
    # Price of the product
    unitPrice: Float!
}

# Input that describes the Prebook Input structure
input PrebookItemInput {
    # The code of the vendor
    vendorCode: String!,
    # Id of the product
    productId: ID!,
    # The Box Type
    boxType: String!,
    # The Unit Type
    unitType: String!,
    # Mark Code of the line
    markCode: String,
    # Price of the product
    unitPrice: Float!
}

